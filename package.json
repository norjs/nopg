{
  "name": "nor-nopg",
  "version": "1.4.4",
  "description": "NoSQL Library for PostgreSQL",
  "bin": {
    "nor-nopg-import": "./tools/import.js",
    "nor-nopg": "./src/bin/nopg.sh"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "./bin/test.sh",
    "test-coverage": "mocha -u bdd -c -R mocha-lcov-reporter tests/test-*.js|./node_modules/coveralls/bin/coveralls.js",
    "test-dot": "mocha -u bdd -c -R dot tests/test-*.js",
    "test-list": "mocha -u bdd -c -R list tests/test-*.js",
    "test-spec": "mocha -u bdd -c -R spec tests/test-*.js",
    "test-progress": "mocha -u bdd -c -R progress tests/test-*.js",
    "test-min": "mocha -u bdd -c -R min tests/test-*.js",
    "test-markdown": "mocha -u bdd -c -R markdown tests/test-*.js",
    "build-tests.md": "npm run test-markdown > tests.md",
    "lint": "node-lint src/ --config=lint.json",
    "jshint": "jshint src/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sendanor/nor-nopg.git"
  },
  "keywords": [
    "nosql",
    "postgresql",
    "node",
    "pg",
    "client"
  ],
  "dependencies": {
    "merge": "^1.1.3",
    "minimist": "^1.1.0",
    "nor-array": "^1.0.0",
    "nor-data": "^1.0.0",
    "nor-debug": "^1.0.0",
    "nor-extend": "^1.0.0",
    "nor-fs": "^1.0.0",
    "nor-function": "^1.0.0",
    "nor-is": "^1.0.0",
    "nor-newrelic": "^1.0.0",
    "nor-pg": "^1.2.0",
    "nor-ref": "^1.0.0",
    "optimist": "^0.6.1",
    "pg-escape": "^0.2.0",
    "q": "^1.0.1",
    "tv4": "^1.0.18",
    "uuid": "^3.2.1"
  },
  "optionalDependencies": {
    "newrelic": "^1.13.2"
  },
  "devDependencies": {
    "async-profile": "^0.4.0",
    "coveralls": "^3.0.0",
    "jscover": "^1.0.0",
    "jscoverage": "^0.6.0",
    "jshint": "^2.9.2",
    "lint": "^1.1.2",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "^1.3.0",
    "nor-pgrunner": "^1.0.0"
  },
  "author": "Jaakko-Heikki Heusala <jheusala@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sendanor/nor-nopg/issues"
  }
}
